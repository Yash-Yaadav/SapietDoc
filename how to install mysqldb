 253  cd /root
 255  wget https://repo.mysql.com/mysql80-community-release-el8-4.noarch.rpm
  256   yum localinstall mysql80-community-release-el8-1.noarch.rpm
  257  yum repolist enabled | grep "mysql.*-community.*"
